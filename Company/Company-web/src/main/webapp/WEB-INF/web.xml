<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>ListCompanyServlet</servlet-name>
        <servlet-class>com.gokisoft.company.servlets.ListCompanyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddCompanyServlet</servlet-name>
        <servlet-class>com.gokisoft.company.servlets.AddCompanyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditCompanyServlet</servlet-name>
        <servlet-class>com.gokisoft.company.servlets.EditCompanyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteCompanyServlet</servlet-name>
        <servlet-class>com.gokisoft.company.servlets.DeleteCompanyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListCompanyServlet</servlet-name>
        <url-pattern>/ListCompanyServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddCompanyServlet</servlet-name>
        <url-pattern>/AddCompanyServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditCompanyServlet</servlet-name>
        <url-pattern>/EditCompanyServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteCompanyServlet</servlet-name>
        <url-pattern>/DeleteCompanyServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
